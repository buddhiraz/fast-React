{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend/build/index.html"
    }
  ],
  "devCommand": "uvicorn main:app --host 0.0.0.0 --port 8000"
}
