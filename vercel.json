{
    "version": 2,
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python-3.8"  
        }
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/main.py"
      }
    ],
    "cleanUrls": true,
    "trailingSlash": false
  }
  